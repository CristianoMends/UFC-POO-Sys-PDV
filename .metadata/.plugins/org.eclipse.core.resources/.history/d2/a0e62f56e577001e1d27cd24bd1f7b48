package pdv.controller;
import java.util.HashMap;
import java.util.Map;
import pdv.model.*;

public class Pdv {
	
	Map<Integer,Produto> produtos;
	Map<Integer, Cliente> clientes;
	Map<Integer, Venda> vendas;
	Map<Integer, Caixa> caixas;
	Map<Integer, Funcionario> funcionarios;
	
	public Pdv() {
		produtos = new HashMap<Integer,Produto>();
	}

}
